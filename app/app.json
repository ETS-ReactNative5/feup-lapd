{
  "expo": {
    "name": "TripLap",
    "slug": "triplap",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Application developed in Markup Languages and Document Processing course at FEUP",
    "githubUrl": "https://github.com/nunottlopes/feup-lapd"
  }
}
